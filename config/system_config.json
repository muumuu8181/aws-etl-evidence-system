{
  "system": {
    "name": "AWS ETL Evidence System",
    "version": "0.18",
    "description": "完全自動化されたETLパイプラインと証跡監視システム"
  },
  "aws": {
    "account_id": "275884879886",
    "region": "ap-northeast-1",
    "profile": "default"
  },
  "step_functions": {
    "sf1": {
      "name": "CSV→Redshift パイプライン",
      "state_machine_name": "sf-etl-observer-dev-ingest",
      "description": "S3 CSV → Glue → Parquet → Redshift"
    },
    "sf2": {
      "name": "JSON→DynamoDB パイプライン",
      "state_machine_name": "sf-json-processor-dev-ingest", 
      "description": "JSON前処理 → DynamoDB書き込み"
    },
    "sf3": {
      "name": "ログ→Athena パイプライン",
      "state_machine_name": "sf-log-processor-dev-ingest",
      "description": "ログ収集 → Glue Crawler → Athena実行"
    }
  },
  "lambda_functions": {
    "monitoring": "lm-etl-observer-dev-collector"
  },
  "s3_buckets": {
    "landing": "etl-observer-dev-landing",
    "staging": "etl-observer-dev-staging", 
    "evidence": "etl-observer-dev-evidence"
  }
}