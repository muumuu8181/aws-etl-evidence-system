{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:ListBucket",
                "s3:GetBucketLocation"
            ],
            "Resource": [
                "arn:aws:s3:::etl-observer-dev-staging",
                "arn:aws:s3:::etl-observer-dev-staging/*",
                "arn:aws:s3:::etl-observer-dev-landing",
                "arn:aws:s3:::etl-observer-dev-landing/*"
            ]
        }
    ]
}